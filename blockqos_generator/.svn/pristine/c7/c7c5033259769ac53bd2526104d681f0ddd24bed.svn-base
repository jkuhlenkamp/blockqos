package com.ibm.block.generator.impl.physical.pm;

import com.ibm.block.generator.impl.PMFactory;
import com.ibm.block.generator.impl.physical.ro.SmallCpuROfferFactory;
import com.ibm.block.generator.impl.physical.ro.SmallRamROfferFactory;
import com.ibm.block.model.core.impl.PlacementModel;

public class SmallPMFactory extends PMFactory {

	public SmallPMFactory(PlacementModel pm) {
		super( pm );
		super.addROfferFactory(new SmallCpuROfferFactory(pm));
		super.addROfferFactory(new SmallRamROfferFactory(pm));
	}
}
