package com.ibm.block.generator.impl.physical.pm;

import com.ibm.block.generator.impl.PMFactory;
import com.ibm.block.generator.impl.physical.ro.LargeCpuROfferFactory;
import com.ibm.block.generator.impl.physical.ro.LargeRamROfferFactory;
import com.ibm.block.model.core.impl.PlacementModel;

public class LargePMFactory extends PMFactory {
	
	public LargePMFactory(PlacementModel pm) {
		super( pm );
		super.addROfferFactory(new LargeCpuROfferFactory(pm));
		super.addROfferFactory(new LargeRamROfferFactory(pm));
	}

}
