  booktitle = {Proceedings of the 15th Asia-Pacific Conference on Software Engineering
	(APSEC)},
  year = {2008},
  series = {IEEE Conference Proceedings},
  pages = {187--194},
  address = {Washington District of Columbia USA},
  publisher = {IEEE Computer Society},
  abstract = {A software product line (SPL) enables stakeholders to derive different
	software products for a domain while providing a high degree of reuse
	of their code units. Software products are derived in a configuration
